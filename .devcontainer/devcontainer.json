{
	"name": "Existing Dockerfile",
	"build": {
		"context": "..",
		"dockerfile": "../Dockerfile"
	},
	"customizations": {
		"vscode": {
			"extensions": [
				"ms-vscode.cpptools",
				"ziglang.vscode-zig",
				"formulahendry.code-runner"
			],
			"settings": {
				"code-runner.enableAppInsights": false,
				"zig.zls.enabled": "on",
				"zig.version": "0.13.0"
			}
		}
	}
}